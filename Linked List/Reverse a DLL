public class Solution {
    public static class ListNode {
        int data;
        ListNode next;
        ListNode prev;

        ListNode(int data) {
            this.data = data;
            this.next = null;
            this.prev = null;
        }
    }

    public ListNode solve(ListNode head) {
        if (head == null || head.next == null) {
            return head;
        }
        ListNode prev = null;
        ListNode temp = head;
        ListNode front;
        while (temp != null) {
            front = temp.next;
            temp.next = prev;
            temp.prev = front;
            prev = temp;
            temp = front;
        }
        return prev;
    }

}
